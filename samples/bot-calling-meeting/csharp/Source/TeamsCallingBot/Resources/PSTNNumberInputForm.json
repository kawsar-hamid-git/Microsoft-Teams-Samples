{
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.4",
  "type": "AdaptiveCard",
  "body": [
    {
      "type": "TextBlock",
      "text": "${inputLabel}"
    },
    {
      "type": "Input.Text",
      "id": "pstnNumberInputId",
      "placeholder": "enter pstn number",
      "maxLength": 100
    }
  ],
  "actions": [
    {
      "type": "Action.Submit",
      "title": "${action}",
      "data": {
        "action": "${action}",
        "msteams": {
          "type": "task/submit"
        },
        "callId": "${callId}"
      }
    }
  ]
}
